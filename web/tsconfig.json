{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNext",
    "composite": false,
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@assets/*": [
        "frame/assets/*"
      ],
      "@styles/*": [
        "frame/styles/*"
      ],
      "@router": [
        "frame/router"
      ],
      "@types/*": [
        "frame/types/*"
      ],
      "@hooks": [
        "frame/hooks"
      ],
      "@component/*": [
        "frame/component/*"
      ],
      "@layout/*": [
        "frame/layout/*"
      ],
      "@atom/*": [
        "atom/*"
      ]
    }
  },
  "include": [
    "frame/**/*.ts",
    "frame/**/*.tsx",
    "frame/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}